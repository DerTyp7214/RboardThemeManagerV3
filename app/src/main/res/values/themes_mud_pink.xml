    <!-- Base application theme. -->
    <style name="Theme.RboardThemeManagerV3_mud_pink" parent="Theme.RboardThemeManager.Colors_mud_pink">
        <item name="android:windowLightStatusBar">?attr/isLightTheme</item>

        <item name="fontFamily">@font/google_sans_text_400</item>

        <item name="android:statusBarColor">?android:attr/colorBackground</item>
        <item name="android:navigationBarColor">@android:color/black</item>

        <item name="floatingActionButtonStyle">?floatingActionButtonSecondaryStyle</item>
        <item name="popupMenuStyle">@style/PopupStyle</item>
        <item name="alertDialogTheme">@style/Dialog.RboardThemeManagerV3.MaterialAlertDialog</item>
        <item name="materialAlertDialogTheme">
            @style/Dialog.RboardThemeManagerV3.MaterialAlertDialog
        </item>

        <item name="collapsingToolbarLayoutStyle">@style/Widget.CollapsingToolbar</item>

        <item name="snackbarStyle">@style/Widget.App.Snackbar</item>
        <item name="snackbarTextViewStyle">@style/Widget.App.SnackbarTextView</item>
        <item name="snackbarButtonStyle">@style/Widget.App.SnackbarButton</item>
    </style>

    <style name="Theme.RboardThemeManagerV3.Main_mud_pink" parent="Theme.RboardThemeManagerV3_mud_pink" />

    <style name="Theme.RboardThemeManagerV3.Info_mud_pink" parent="Theme.RboardThemeManagerV3_mud_pink">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RboardThemeManagerV3.InstallPack_mud_pink" parent="Theme.RboardThemeManagerV3_mud_pink">
        <item name="android:statusBarColor">?attr/colorSurfaceVariant</item>
    </style>

    <style name="Theme.RboardThemeManagerV3.ShareFlags_mud_pink" parent="Theme.RboardThemeManagerV3_mud_pink">
